<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>rest-api</name>
    <display-name>REST API</display-name>
    <version>@Plugin_Version@</version>
    <description>Exposes TeamCity API via REST-like protocol. The plugin is essential for TeamCity Web UI.</description>

    <vendor>
      <name>JetBrains</name>
      <url>http://www.jetbrains.com</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
  <parameters>
    <parameter name="api.path">/app/rest/2018.1,/app/rest,/app/rest/latest,/app/rest/ui,/api,/app/graphql</parameter>
    <parameter name="api.version">2018.1</parameter>
  </parameters>
</teamcity-plugin>
