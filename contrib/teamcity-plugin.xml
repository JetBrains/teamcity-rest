<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>rest-api-contrib</name>
    <display-name>REST API Contrib</display-name>
    <version>@Plugin_Version@</version>
    <description>Additionals for TeamCity API REST-like protocol</description>

    <vendor>
      <name>JetBrains</name>
      <url>http://www.jetbrains.com</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
  <dependencies>
    <plugin name="rest-api"/>
  </dependencies>
</teamcity-plugin>
