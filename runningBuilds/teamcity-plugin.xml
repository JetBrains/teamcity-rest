                 xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>rest-api-runningBuilds</name>
    <display-name>REST API runningBuilds</display-name>
    <version>@Plugin_Version@</version>
    <description>Addition for TeamCity REST API related to agent-less builds</description>

    <vendor>
      <name>JetBrains, s.r.o.</name>
      <url>http://www.jetbrains.com</url>
